{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/contribution-report/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nxext/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/contribution-report",
        "baseHref": "/",
        "configFile": "@nxext/vite/plugins/vite",
        "frameworkConfigFile": "@nxext/react/plugins/vite"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/contribution-report/src/environments/environment.ts",
              "with": "apps/contribution-report/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nxext/vite:dev",
      "options": {
        "outputPath": "dist/apps/contribution-report",
        "baseHref": "/",
        "configFile": "@nxext/vite/plugins/vite",
        "frameworkConfigFile": "@nxext/react/plugins/vite"
      },
      "configurations": {
        "production": {
          "buildTarget": "contribution-report:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/contribution-report/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/contribution-report"],
      "options": {
        "jestConfig": "apps/contribution-report/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "inputs": [
        {
          "env": "CLOUDFLARE_ACCOUNT_ID"
        },
        {
          "env": "CLOUDFLARE_API_TOKEN"
        }
      ],
      "dependsOn": [
        {
          "target": "build:production",
          "projects": "self"
        }
      ],
      "options": {
        "command": "npx -y wrangler pages publish ./dist/apps/contribution-report --project-name=contribution-report --branch=main",
        "color": true
      }
    },
    "deployStaging": {
      "executor": "@nrwl/workspace:run-commands",
      "inputs": [
        {
          "env": "CLOUDFLARE_ACCOUNT_ID"
        },
        {
          "env": "CLOUDFLARE_API_TOKEN"
        }
      ],
      "dependsOn": [
        {
          "target": "build:production",
          "projects": "self"
        }
      ],
      "options": {
        "command": "npx -y wrangler pages publish ./dist/apps/contribution-report --project-name=contribution-report-staging --branch=main",
        "color": true
      }
    }
  },
  "tags": []
}
